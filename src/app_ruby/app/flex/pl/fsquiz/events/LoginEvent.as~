package pl.fsquiz.events {
    import flash.events.Event;
                                                
    public class LoginEvent extends Event {
                                                     
        public static const LOGIN:String = "login";
                                 D
        public var user:XML;
                                                   
        public function LoginEvent(user:XML) {
                              
            super(LOGIN);
                                  
            this.user = user;
        }
    }
}

